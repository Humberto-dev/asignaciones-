{{ form_start(form, {'attr': {'id': id|default('form')}} )}}
    {{ form_widget(form) }}
        {% if with_submit is not defined or with_submit == true %}
            <input type="buttom" value="{{'Delete user'|trans}}" class="btn btn-lg btn-block btn-danger"
                onclick="confirmDelte()">
        {% endif %}
{{ form_end(form) }}

<script>
    var message= '{{ message }}'

    function confirmDelte()
    {
        bootbox.confirm(message, function(result){
            if(result == true){
                document.form.submit();
            }
        });
    }
</script>